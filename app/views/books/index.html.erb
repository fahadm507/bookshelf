<div class="row">
  <div class="col-lg-1 text-left">
    <h1>All books</h1>
    <a class="btn-success btn-large" href="/books/new">Add A New Book</a>
    <ul class="list">
      <% @books.each do |book | %>
        <li class="list-group-item"><a href="/books/<%= book.id %>"><%= book.title %></a>
      <%end%>
    </ul>
  </div>
</div>
